<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:orientation="vertical"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             tools:layout_editor_absoluteY="97dp"
                                             tools:layout_editor_absoluteX="16dp">

    <TextView
        android:id="@+id/Exercise2Lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"
        android:background="@drawable/featuregreen"
        android:gravity="center_vertical"
        android:text="reps"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Exercise1Lbl"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <TextView
        android:id="@+id/Exercise1Lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"
        android:background="@drawable/featuregreen"
        android:gravity="center_vertical"
        android:text="test"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/StructureLbl"/>

    <TextView
        android:id="@+id/Exercise3lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"
        android:background="@drawable/featuregreen"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Exercise2Lbl"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/Exercise4Lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"
        android:background="@drawable/featuregreen"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Exercise3lbl"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/Exercise5Lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"
        android:background="@drawable/featuregreen"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Exercise4Lbl"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/reps1Lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"

        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/Exercise1Lbl"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/StructureLbl"/>
    <TextView
        android:id="@+id/reps2Lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"

        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/Exercise2Lbl"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/reps1Lbl"/>

    <TextView
        android:id="@+id/reps3lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"

        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/Exercise3lbl"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/reps2Lbl"/>

    <TextView
        android:id="@+id/reps4lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"

        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/Exercise4Lbl"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/reps3lbl"/>

    <TextView
        android:id="@+id/reps5lbl"
        android:layout_width="127dp"
        android:layout_height="40dp"

        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/Exercise5Lbl"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/reps4lbl"/>


    <Button
        android:id="@+id/completeButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_green_dark"
        android:text="Workout Completed"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="86dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.0"/>

    <TextView
        android:id="@+id/StructureLbl"
        android:layout_width="336dp"
        android:layout_height="43dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/header"
        android:gravity="center_vertical|center_horizontal"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>